{
  "name": "expresslanets",
  "packageManager": "yarn@4.2.2",
  "type": "module",
  "scripts": {
    "build": "yarn tsc",
    "dev": "yarn nodemon",
    "lint": "yarn eslint . --ext .ts,.js",
    "prettier": "yarn prettier --parser typescript --write ./src"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.4",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "eslint-define-config": "^2.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "yarn ts-node src/index.ts"
  }
}
